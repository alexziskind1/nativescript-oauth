{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "experimentalDecorators": true,
        "noLib": false,
        "preserveConstEnums": true,
        "declaration": false,
        "noEmitHelpers": true,
        "suppressImplicitAnyIndexErrors": true,
        "lib": [
            "es6",
            "dom"
        ],
        "baseUrl": ".",
        "paths": {
            "*": [
                "./node_modules/tns-core-modules/*",
                "./node_modules/*"
            ]
        }
    },
    "files": [
        "auth-helper.ts",
        "auth-helper-facebook.ts",
        "auth-helper-google.ts",
        "auth-helper-office365.ts",
        "auth-helper-uaa.ts",
        "auth-helper-linkedin.ts",
        "auth-helper-dropbox.ts",
        "index.ts",
        "tns-oauth.ts",
        "tns-oauth-page-provider.ts",
        "tns-oauth-token-cache.ts",
        "tns-oauth-utils.ts",
        "tns-oauth-webview-helper.ios.ts",
        "tns-oauth-webview-helper.android.ts",
        "./typings/querystring.d.ts",
        "./typings/url.d.ts"
    ],
    "compileOnSave": false,
    "exclude": [
        "node_modules",
        "platforms"
    ]
}